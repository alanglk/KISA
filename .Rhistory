#########################################################################
#                         REPASO ÁLGEBRA LINEAL                   #
#########################################################################
Hn <- function(n)
{
# Matriz de centrado (dim. n)
unos <- matrix(rep(1, n), ncol=1)
H <- diag(1, n) - unos%*%t(unos)/n
return(H)
}
